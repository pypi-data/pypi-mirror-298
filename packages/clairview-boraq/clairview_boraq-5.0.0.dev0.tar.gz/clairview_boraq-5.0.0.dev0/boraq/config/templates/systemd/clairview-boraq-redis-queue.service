[Unit]
Description="{{ boraq_name }}-redis-queue"
PartOf={{ boraq_name }}-redis.target

[Service]
User={{ user }}
Group={{ user }}
Restart=always
ExecStart={{ redis_server }} {{ redis_queue_config }}
StandardOutput=file:{{ boraq_dir }}/logs/redis-queue.log
StandardError=file:{{ boraq_dir }}/logs/redis-queue.error.log
WorkingDirectory={{ sites_dir }}
