[Unit]
Description="{{ boraq_name }}-clairview-short-worker %I"
PartOf={{ boraq_name }}-workers.target

[Service]
User={{ user }}
Group={{ user }}
Restart=always
ExecStart={{ boraq_cmd }} worker --queue short
StandardOutput=file:{{ boraq_dir }}/logs/worker.log
StandardError=file:{{ boraq_dir }}/logs/worker.error.log
WorkingDirectory={{ boraq_dir }}
