[Unit]
Description="{{ boraq_name }}-redis-cache"
PartOf={{ boraq_name }}-redis.target

[Service]
User={{ user }}
Group={{ user }}
Restart=always
ExecStart={{ redis_server }} {{ redis_cache_config }}
StandardOutput=file:{{ boraq_dir }}/logs/redis-cache.log
StandardError=file:{{ boraq_dir }}/logs/redis-cache.error.log
WorkingDirectory={{ sites_dir }}
